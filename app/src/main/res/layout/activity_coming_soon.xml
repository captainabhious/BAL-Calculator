<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_coming_soon"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/colorBack"
    tools:context="com.example.nathan_mccauslin.bloodalcoholcalculator.ComingSoonActivity">

    <TextView
        android:id="@+id/firstHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="No history to show yet!"
        android:textColor="@color/colorAccent" />

    <TextView
        android:layout_width="fill_parent"
        android:id="@+id/secondHistory"
        android:layout_marginTop="50sp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorAccent"/>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100sp"
        android:id="@+id/thirdHistory"
        android:textColor="@color/colorAccent"/>

    <TextView
        android:layout_width="fill_parent"
        android:id="@+id/fourthHistory"
        android:layout_marginTop="150sp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorAccent"/>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="200sp"
        android:id="@+id/fifthHistory"
        android:textColor="@color/colorAccent"/>


</RelativeLayout>
